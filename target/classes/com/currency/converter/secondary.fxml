<VBox alignment="CENTER" prefHeight="383.0" prefWidth="598.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.currency.converter.controller.SecondaryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label prefHeight="39.0" prefWidth="267.0" text="Converter Panel">
         <font>
            <Font name="Leelawadee UI" size="38.0" />
         </font>
      </Label>
      <GridPane prefHeight="252.0" prefWidth="558.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="322.0" minWidth="10.0" prefWidth="310.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="374.0" minWidth="10.0" prefWidth="248.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ComboBox prefHeight="41.0" prefWidth="238.0" promptText="Choose the local currency " visibleRowCount="5" GridPane.columnIndex="1" />
            <ComboBox prefHeight="37.0" prefWidth="248.0" promptText="choose the foreign currency " GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField prefHeight="40.0" prefWidth="310.0" />
            <TextField prefHeight="37.0" prefWidth="310.0" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button fx:id="SecondaryButton" text="Main menu" onAction="#switchToPrimary" />
   </children>
</VBox>
